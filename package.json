    "test": "jest"
  },
  "keywords": [
    "booru",
    "e621",
    "hypnohub",
    "danbooru",
    "konachan",
    "yandere",
    "gelbooru",
    "rule34",
    "safebooru",
    "lolibooru",
    "sakugabooru",
    "3dbooru",
    "tbib",
    "xbooru",
    "paheal",
    "derpibooru"
