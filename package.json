  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "fast-xml-parser": "4.0.11",
    "isomorphic-unfetch": "3.1.0"
  },
  "devDependencies": {
    "@types/jest": "29.2.2",
    "@types/node": "16.9.0",
    "@types/node-fetch": "2.6.2",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@typescript-eslint/parser": "5.42.1",
    "chalk": "5.1.2",
    "enquirer": "2.3.6",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.6.2",
    "execa": "6.1.0",
    "jest": "29.3.1",
    "minimist": "1.2.7",
    "semver": "7.3.8",
    "tsup": "6.4.0",
    "typescript": "4.8.4"
  },
  "directories": {
    "lib": "src/",
    "test": "test/"
  },
  "contributors": [
